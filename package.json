{
  "name": "componente-avancado",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/compat": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "vite": "^4.0.5",
    "vue": "^3.1.0"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.1.0",
    "eslint": "8",
    "eslint-plugin-vue": "8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
